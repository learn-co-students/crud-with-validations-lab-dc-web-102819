
<%= form_for @song do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<%= f.label :artist_name %>
	<%= f.text_field :artist_name %>
	<%= f.label :released %>
	<%= f.check_box :released %>
	<%= f.label :release_year %>
	<%= f.number_field :release_year, in: 1200...2020 %>
	<%= f.label :genre %>
	<%= f.text_field :genre %>
	<%= f.submit submit_text %>
<%end%>